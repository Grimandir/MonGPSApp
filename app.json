{
    "expo": {
      "name": "MonGPSApp",
      "slug": "mongpsapp",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/icons/logo.png",
      "userInterfaceStyle": "automatic",
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "updates": {
        "fallbackToCacheTimeout": 0
      },
      "assetBundlePatterns": [
        "**/*"
      ],
      "permissions": [
        "ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION", "FOREGROUND_SERVICE"
      ],

      "ios": {
        "supportsTablet": true,
        "bundleIdentifier": "com.votrenom.mongpsapp"
      },
      "android": {
        "package": "com.votrenom.mongpsapp",
        "adaptiveIcon": {
          "foregroundImage": "./assets/icons/logo.png",
          "backgroundColor": "#FFFFFF"
        }
      },
      "web": {
        "favicon": "./assets/icons/favicon.png"
      },
      "plugins": [
        [
          "expo-location",
          {
            "locationAlwaysAndWhenInUsePermission": "Nous utilisons votre localisation pour la navigation GPS."
          }
        ]
      ]
    }
  }
  